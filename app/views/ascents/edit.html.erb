<h1>Editing Ascent</h1>

<%= render 'form', ascent: @ascent %>

<% if @mountain then %>
	<%= link_to ['Volver a', @mountain.fullname].join(" "), mountain_path(@mountain), class: "btn btn-secondary my-2" %>
<% else %>
	<%= link_to 'Volver a ascensos', ascents_path, class: "btn btn-secondary my-2" %>
<% end %>