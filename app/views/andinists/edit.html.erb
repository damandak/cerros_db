<% content_for :meta_title, "#{@andinist.fullname} | #{DEFAULT_META["meta_title"]}" %>
<% if can? :manage, Andinist %>
	<h1>Editar Andinista</h1>

	<%= render 'form', andinist: @andinist %>

	<%= link_to ['Volver a', @andinist.fullname].join(" "), @andinist %> |
	<%= link_to 'Volver a Andinistas', andinists_path %>
<% else %>
	<p> No tienes acceso a este sitio </p>
<% end %>
